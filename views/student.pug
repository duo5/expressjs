extends ../layout/common.pug
block content

    form(action="/student/search", method="get")
        div(class="col")
            input(class="form-control",type="text",name="name",value=key)
        div(class="col")
            button(class="btn btn-primary") Search
    

    ul
        each s in students
            li
                span= s.name
                a(href="/student/detail/"+s.id) Detail

    a(href="/student/add") add
